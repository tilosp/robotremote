# https://raw.githubusercontent.com/github/gitignore/master/Java.gitignore
*.class

# Mobile Tools for Java (J2ME)
.mtj.tmp/

# Package Files #
*.jar
*.war
*.ear

# virtual machine crash logs, see http://www.java.com/en/download/help/error_hotspot.xml
hs_err_pid*


# https://raw.githubusercontent.com/github/gitignore/master/Gradle.gitignore
.gradle
build/

# Ignore Gradle GUI config
gradle-app.setting

# Avoid ignoring Gradle wrapper jar file (.jar files are usually ignored)
!gradle-wrapper.jar

# Cache of project
.gradletasknamecache

# # Work around https://youtrack.jetbrains.com/issue/IDEA-116898
# gradle/wrapper/gradle-wrapper.properties

# https://raw.githubusercontent.com/github/gitignore/e96a988a4e24535841a5fb94952ac1727ce5bbf5/Android.gitignore
# Built application files
*.apk
*.ap_

# Files for the ART/Dalvik VM
*.dex

# Generated files
bin/
gen/
out/

# Local configuration file (sdk path, etc)
local.properties

# Proguard folder generated by Eclipse
proguard/

# Log Files
*.log

# Android Studio Navigation editor temp files
.navigation/

# Android Studio captures folder
captures/

# Intellij
*.iml

# Keystore files
*.jks
